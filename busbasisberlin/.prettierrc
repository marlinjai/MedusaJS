{
	"printWidth": 80,
	"singleQuote": true,
	"trailingComma": "all",
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"tabWidth": 2,
	"useTabs": true,
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"semi": true,
	"insertPragma": false,
	"proseWrap": "preserve",
	"jsxSingleQuote": false
}
